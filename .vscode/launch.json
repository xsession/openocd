{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug OpenOCD (gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build-vscode/src/openocd",
      "args": [
        "-s",
        "${workspaceFolder}/tcl",
        "-f",
        "board/stm32f4discovery.cfg"
      ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "externalConsole": false,
      "preLaunchTask": "openocd: build (debug)",
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Debug OpenOCD (gdb, Windows .exe)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build-vscode/src/openocd.exe",
      "args": [
        "-s",
        "${workspaceFolder}/tcl",
        "-f",
        "board/stm32f4discovery.cfg"
      ],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "externalConsole": false,
      "preLaunchTask": "openocd: build (debug)",
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
